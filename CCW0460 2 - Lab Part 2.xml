  &lt;/div&gt;
  &lt;sp-widget ng-if="data.dataTableWidget" widget="data.dataTableWidget"&gt;&lt;/sp-widget&gt;
&lt;/div&gt;]]&gt;&lt;/template&gt;&lt;/sp_widget&gt;</payload><sys_class_name>sp_rel_widget_clone</sys_class_name><sys_created_by>nicholas.yancer</sys_created_by><sys_created_on>2018-03-14 19:37:50</sys_created_on><sys_id>92498fde4ff01b402debb7a18110c714</sys_id><sys_mod_count>0</sys_mod_count><sys_name>da494b5e4ff01b402debb7a18110c724</sys_name><sys_package display_value="Global" source="global">global</sys_package><sys_policy/><sys_scope display_value="Global">global</sys_scope><sys_update_name>sp_rel_widget_clone_92498fde4ff01b402debb7a18110c714</sys_update_name><sys_updated_by>nicholas.yancer</sys_updated_by><sys_updated_on>2018-03-14 19:37:50</sys_updated_on></sp_rel_widget_clone></record_update>]]></payload>
