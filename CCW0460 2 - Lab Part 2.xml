<name>sys_ui_script_17ca3e564ff01b402debb7a18110c70e</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sys_ui_script"&gt;&lt;sys_ui_script action="INSERT_OR_UPDATE"&gt;&lt;active&gt;true&lt;/active&gt;&lt;description/&gt;&lt;global&gt;false&lt;/global&gt;&lt;name&gt;toggleSwitch&lt;/name&gt;&lt;script&gt;&lt;![CDATA[angular.module('css-toggle-switch',[]).directive('toggleSwitch',[function(){
	
	var defaults = {
		labelTrue: 'On',
		labelFalse: 'Off'
	};
	
	return {
		restrict: 'E',
		scope: {
			watchVar: '=',
			title: '@'
		},
		template: '&lt;div&gt;' +
		'&lt;label class="switch-light" onclick=""&gt;' +
		'&lt;input type="checkbox" ng-model="watchVar"&gt;' +
		'&lt;strong&gt;' +
		'{{ title }}' +
			'&lt;/strong&gt;' +
			'&lt;span class="alert alert-light"&gt;' +
			'&lt;span&gt;{{ labelFalse }}&lt;/span&gt;' +
				'&lt;span&gt;{{ labelTrue }}&lt;/span&gt;' +
					'&lt;a class="btn btn-primary"&gt;&lt;/a&gt;' +
					'&lt;/span&gt;' +
					'&lt;/label&gt;' +
					'&lt;/div&gt;' +
					'&lt;style&gt;' +
					'.switch-light {' +
						' max-width: 125px; '+
						'}' +
						'.switch-light .alert-light {' +
							'      border: 1px solid #e2e2e2;' +
							'}' +
							'&lt;/style&gt;',
		compile: function(){
			return {
				pre: function(scope,el,attrs){
					// Set defaults for labels if nothing passed into the directive
					scope.labelTrue = attrs.labelTrue || defaults.labelTrue;
					scope.labelFalse = attrs.labelFalse || defaults.labelFalse;
				},
				post: function(scope,el,attrs){
					
				}
			};
		}
	};
}]);]]&gt;&lt;/script&gt;&lt;script_name/&gt;&lt;sys_class_name&gt;sys_ui_script&lt;/sys_class_name&gt;&lt;sys_created_by&gt;nicholas.yancer&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2018-03-14 18:45:08&lt;/sys_created_on&gt;&lt;sys_id&gt;17ca3e564ff01b402debb7a18110c70e&lt;/sys_id&gt;&lt;sys_mod_count&gt;16&lt;/sys_mod_count&gt;&lt;sys_name&gt;toggleSwitch&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_ui_script_17ca3e564ff01b402debb7a18110c70e&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;nicholas.yancer&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2018-03-14 20:06:37&lt;/sys_updated_on&gt;&lt;use_scoped_format&gt;false&lt;/use_scoped_format&gt;&lt;/sys_ui_script&gt;&lt;/record_update&gt;</payload>
